@EndUserText.label : 'Tabela - Preços por tipo de veícul'
@AbapCatalog.enhancement.category : #NOT_EXTENSIBLE
@AbapCatalog.tableCategory : #TRANSPARENT
@AbapCatalog.deliveryClass : #A
@AbapCatalog.dataMaintenance : #ALLOWED
define table zvtdt_preco {

  key mandt        : mandt not null;
  key id_preco     : zvtde_id_preco not null;
  @AbapCatalog.foreignKey.keyType : #KEY
  @AbapCatalog.foreignKey.screenCheck : true
  key tipo_veiculo : zvtde_tipo_veic not null
    with foreign key [1,1] zvtdt_veiculo
      where mandt = zvtdt_preco.mandt
        and tipo_veiculo = zvtdt_preco.tipo_veiculo;
  @Semantics.amount.currencyCode : 'zvtdt_preco.moeda'
  preco            : zvtde_preco;
  moeda            : zvtde_moeda;
  @Semantics.amount.currencyCode : 'zvtdt_preco.moeda'
  multa            : zvtde_multa;

}
